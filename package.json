{
    "name": "form-to-csv",
    "version": "1.0.0",
    "type": "module",
    "description": "A Node.js server to handle form submissions and append them to a CSV file.",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "node --watch server.js",
        "dev-env": "node --watch server.js --env-file .env",
        "mongo": "docker run --rm -d -p 27017:27017 -v mongo-v:/data/db --name mongo-v mongo",
        "mongo:stop": "docker stop mongo-v"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "exceljs": "^4.4.0",
        "express": "^4.17.1",
        "mongoose": "^8.3.4"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/mongodb": "^4.0.7",
        "typescript": "^5.4.5"
    }
}
